    <section id="pauline_narcisique" style="position:fixed; bottom:0;right:0;width: 25%;">


    <style>
    @font-face {
        font-family: 'Amatic SC';
        font-style: normal;
        font-weight: 400;
        src: local('Amatic SC Regular'), local('AmaticSC-Regular'), url(http://themes.googleusercontent.com/static/fonts/amaticsc/v3/DPPfSFKxRTXvae2bKDzp5D8E0i7KZn-EPnyo3HZu7kw.woff) format('woff');
    }

    @font-face {
        font-family: 'Amatic SC';
        font-style: normal;
        font-weight: 700;
        src: local('Amatic SC Bold'), local('AmaticSC-Bold'), url(http://themes.googleusercontent.com/static/fonts/amaticsc/v3/IDnkRTPGcrSVo50UyYNK73hCUOGz7vYGh680lGh-uXM.woff) format('woff');
    }

        /* Reset ======================================================= */


    .newsflash {
        font-family: sans-serif;
        background: #313455;
        color: wheat;
        position: fixed;
        bottom: 0;
        left: 0;
        right: 0;
        text-align: center;
        padding: 1rem;
        z-index: 10000;
        opacity: .85;
        transition: .3s;
    }
    .newsflash em { font-style: normal; font-family: sans-serif; color: tomato; font-weight: bold; }
    .newsflash:hover span { text-decoration: underline; }
    .newsflash:hover {
        opacity: 1;
        color: white;
    }



    .pauline_narcisique  * {
        margin: 0;
        padding: 0;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
    }

    .pauline_narcisique header,
    .pauline_narcisique footer,
    .pauline_narcisique section,
    .pauline_narcisique figure,
    .pauline_narcisique figcaption {
        display: block;
    }

    .pauline_narcisique  {
        font: 300 100%/1.5 'Amatic SC', cursive;
        color: #494636;
        background: #F8EEB9;
        overflow-y: scroll;
        overflow-x: hidden;
    }

    .pauline_narcisique section {
        width: 290px;
        margin: 0 auto;
    }

    .pauline_narcisique  a{
        cursor: pointer;
        text-decoration: none;
    }

    .pauline_narcisique .paused * {
        -webkit-animation-play-state: paused;
        animation-play-state: paused;
        opacity: .5;
        pointer-events: none;
        -webkit-transition: opacity .4s;
        transition: opacity .4s;
    }

        /* Creature container ========================================== */

    .creatures {
        list-style: none;
        margin: 15px 0;
        -webkit-transition: opacity .3s;
        transition: opacity .3s;
        text-align: center;
    }

    .creatures li {
        display: inline-block;
        vertical-align: top;
        text-align: center;
        width: 260px;
        list-style: none;
        margin: 0 auto;
        padding: 15px 30px;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
    }

    .pauline_narcisique .error {
        padding: 45px;
        font-size: 2.5rem;
        font-weight: bold;
    }

        /* .pauline_narcisique  header ====================================================== */

    .pauline_narcisique hgroup {
        text-align: center;
        white-space: nowrap;
    }

    .pauline_narcisique .logo,
    .make {
        position: relative;
        display: block;
        color: #494636;
    }

    .pauline_narcisique .logo {
        font-size: 4rem;
    }

    .make {
        font-size: 2rem;
    }

    .pauline_narcisique  header {
        display: none;
        color: #F8EEB9;
        font-weight: 700;
        width: 100%;
        line-height: 2;
        background: #3B382B center bottom no-repeat;
        background-image: -webkit-linear-gradient(top, transparent 0%, rgba(0,0,0,.05) 40%, rgba(0,0,0,.3) 100%);
        background-image: linear-gradient(to bottom, transparent 0%, rgba(0,0,0,.05) 40%, rgba(0,0,0,.3) 100%);
        background-size: 100% 45px;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
    }

    .pauline_narcisique  header ul {
        list-style: none;
    }

    .pauline_narcisique  header li {
        text-align: left;
        font: 400 1em "Helvetica", sans-serif;
        background: rgba(0,0,0,.4);
        padding: 10px 15px;
        margin-top: .4em;
        border-radius: 5px;
        color: #DAD1A4;
        -webkit-user-select: text;
        -moz-user-select: text;
        -ms-user-select: text;
        user-select: text;
    }

    .pauline_narcisique  i{
        font-style: normal;
        border-bottom: 2px solid transparent;
        -webkit-transition: .2s;
        transition: .2s;
    }

    .pauline_narcisique .active {
        -webkit-animation: pulse 1s infinite;
        animation: pulse 1s infinite;
    }

    @-webkit-keyframes pulse {
        50% {
            color: #fff;
            border-bottom-color: #DAD1A4;
        }
    }

    @keyframes pulse {
        50% {
            color: #fff;
            border-bottom-color: #DAD1A4;
        }
    }

    .pauline_narcisique  header section {
        width: 100%;
        text-align: center;
        padding: 30px;
    }

    .pauline_narcisique  header a {
        color: #F8EEB9;
        border-bottom: 2px solid #67624C;
        -webkit-transition: border-color .2s;
        transition: border-color .2s;
    }

    .pauline_narcisique  header a:hover {
        border-bottom-color: #F8EEB9;
    }

    .pauline_narcisique  header h2 {
        font-size: 1em;
        padding: 15px 30px 0;
        opacity: .8;
    }

        /* .pauline_narcisique  footer ====================================================== */

    .pauline_narcisique  footer {
        display: none;
        text-align: center;
        padding: 60px 30px;
        font-size: 2.3rem;
    }

    .pauline_narcisique  footer a {
        color: #111;
        border-bottom: 1px dashed #111;
    }

    .pauline_narcisique  footer a:hover {
        border-bottom-style: solid;
    }

        /* Creature ==================================================== */

    figure {
        width: 200px;
        height: 200px;
        padding: 40px;
        box-shadow: inset -10px -30px rgba(0,0,0,.06);
        border-radius: 50%;
        -webkit-animation: bobble 5s ease-in-out infinite;
        animation: bobble 5s ease-in-out infinite;
        -webkit-transform-origin: center bottom;
        transform-origin: center bottom;
        -webkit-transition: padding .3s, bottom .3s;
        transition: padding .3s, bottom .3s;
        text-align: center;
        background: cadetblue;
    }

    @-webkit-keyframes bobble {
        33% {
            -webkit-transform: rotate(5deg);
            transform: rotate(5deg);
        }

        66% {
            -webkit-transform: rotate(-5deg);
            transform: rotate(-5deg);
        }
    }

    @keyframes bobble {
        33% {
            -webkit-transform: rotate(5deg);
            transform: rotate(5deg);
        }

        66% {
            -webkit-transform: rotate(-5deg);
            transform: rotate(-5deg);
        }
    }

    .pauline_narcisique li:nth-child(5n+1) figure {
        -webkit-animation-delay: -3s;
        animation-delay: -3s;
        background-color: cadetblue;
    }

    .pauline_narcisique li:nth-child(5n+1) figure {
        -webkit-animation-delay: -3s;
        animation-delay: -3s;
        background-color: tomato;
    }

    .pauline_narcisique  li:nth-child(5n+2) figure {
        -webkit-animation-delay: -1s;
        animation-delay: -1s;
        background-color: dodgerblue;
    }

    .pauline_narcisique li:nth-child(5n+3) figure {
        -webkit-animation-delay: -4s;
        animation-delay: -4s;
        background-color: coral;
    }

    .pauline_narcisique li:nth-child(5n+4) figure {
        -webkit-animation-delay: -.2.5s;
        animation-delay: -.2.5s;
        background-color: limegreen;
    }

    .pauline_narcisique li:hover figure {
        padding: 30px 40px;
    }

    .pauline_narcisique li:hover .click-anim {
        -webkit-transform: translateY(-6px);
        transform: translateY(-6px);
    }

    .click-anim {
        -webkit-transition: .3s cubic-bezier(.15,.60,.30,1.9);
        transition: .3s cubic-bezier(.15,.60,.30,1.9);
        -webkit-transform-origin: center bottom;
        transform-origin: center bottom;
    }

    .pauline_narcisique li:active .click-anim {
        -webkit-transform: scale(1.1);
        transform: scale(1.1);
    }

        /* Captions ==================================================== */

    figcaption {
        padding: 10px 0;
        font-size: 12px;
        font-family: monospace;
    }

    .pauline_narcisique  em{
        font-family: "Georgia", serif;
    }

    figcaption a {
        display: inline-block;
        color: #494636;
        border-bottom: 1px solid transparent;
        white-space: nowrap;
    }

    figcaption a:hover {
        text-decoration: underline;
    }

    time {
        display: block;
    }

    time a {
        font-size: 11px;
    }

        /* Shadow ======================================================= */

    .shadow {
        height: 20px;
        background: rgba(0,0,0,.1);
        width: 60%;
        border-radius: 100%;
        margin: -15px 0 0 30%;
        -webkit-transition: .3s;
        transition: .3s;
    }

    .pauline_narcisique li:hover .shadow {
        background: rgba(0,0,0,.05);
    }

    .pauline_narcisique li:active .shadow {
        background: rgba(0,0,0,.08);
        width: 70%;
    }

        /* Eyes ======================================================== */

    .eye {
        display: inline-block;
        width: 10px;
        height: 15px;
        margin: 0 5px 5px;
        background: rgba(0,0,0,.9);
        border-radius: 10px;
        border-radius: 50%;
        -webkit-animation: blink 4s ease-in-out infinite;
        animation: blink 4s ease-in-out infinite;
    }

    @-webkit-keyframes blink {
        90% {
            -webkit-transform: none;
            transform: none;
        }

        95% {
            -webkit-transform: rotateX(90deg);
            transform: rotateX(90deg);
        }
    }

    @keyframes blink {
        90% {
            -webkit-transform: none;
            transform: none;
        }

        95% {
            -webkit-transform: rotateX(90deg);
            transform: rotateX(90deg);
        }
    }

        /* Moustache =================================================== */

    .moustache {
        display: inline-block;
        width: 100%;
        height: 10px;
    }

    .moustache:before,
    .moustache:after {
        display: inline-block;
        color: rgba(0,0,0,.9);
        content: "~";
        font-size: 4em;
        line-height: 0;
        font: 5em/20px "Helvetica", Arial;
        margin: 0 -4px;
    }

    .moustache:before {
        -webkit-transform: rotateY(180deg);
        transform: rotateY(180deg);
    }

    .pauline_narcisique li:hover .moustache {
        -webkit-animation: moustache-wiggle 2s infinite;
        animation: moustache-wiggle 2s infinite;
    }

    @-webkit-keyframes moustache-wiggle {
        50%, 90% {
            -webkit-transform: none;
            transform: none;
        }

        60%, 80% {
            -webkit-transform: rotate(-5deg);
            transform: rotate(-5deg);
        }

        70% {
            -webkit-transform: rotate(5deg);
            transform: rotate(5deg);
        }
    }

    @keyframes moustache-wiggle {
        50%, 90% {
            -webkit-transform: none;
            transform: none;
        }

        60%, 80% {
            -webkit-transform: rotate(-5deg);
            transform: rotate(-5deg);
        }

        70% {
            -webkit-transform: rotate(5deg);
            transform: rotate(5deg);
        }
    }

        /* Mouth =================================================== */

    .mouth {
        display: block;
        width: 100%;
        height: 20px;
        margin: 5px auto 0;
        background: rgba(0,0,0,.9);
        border-radius: 10px 10px 100px 100px;
        border-radius: 10px 10px 50% 50%;
        -webkit-transition: border-radius .5s, height .5s cubic-bezier(.15,.60,.30,1.9);
        transition: border-radius .5s, height .5s cubic-bezier(.15,.60,.30,1.9);
        overflow: hidden;
        line-height: 0;
        -webkit-transform-origin: center top;
        transform-origin: center top;
    }

    .teeth:after,
    .teeth:before,
    .tooth:after {
        display: inline-block;
        content: "";
        margin: 0 2px;
        width: 15px;
        height: 15px;
        background: #eee;
    }

    .pauline_narcisique li:nth-child(odd) .tooth:after {
        margin-right: 17px;
    }

    .pauline_narcisique li:nth-child(even) .tooth:after {
        margin-left: 17px;
    }

        /* Behaviors =================================================== */

    .hungry .mouth {
        -webkit-animation: hungry .5s infinite;
        animation: hungry .5s infinite;
        border-radius: 30px 30px 50% 50%;
        height: 80px;
    }

    .hungry .mouth:hover {
        cursor: none;
    }

    @-webkit-keyframes hungry {
        40% {
            height: 40px;
        }
    }

    @keyframes hungry {
        40% {
            height: 40px;
        }
    }

    .nervous .mouth {
        -webkit-animation: nervous .2s infinite;
        animation: nervous .2s infinite;
        border-radius: 40% 40% 5px 5px;
    }

    @-webkit-keyframes nervous {
        50% {
            height: 24px;
        }
    }

    @keyframes nervous {
        50% {
            height: 24px;
        }
    }

    .hyper {
        -webkit-animation: hyper 2s infinite;
        animation: hyper 2s infinite;
    }

    @-webkit-keyframes hyper {
        25% {
            -webkit-transform: translateY(-10px);
            transform: translateY(-10px);
        }

        15%, 50% {
            -webkit-transform: rotateX(20deg);
            transform: rotateX(20deg);
        }

        75% {
            -webkit-transform: none;
            transform: none;
        }
    }

    @keyframes hyper {
        25% {
            -webkit-transform: translateY(-10px);
            transform: translateY(-10px);
        }

        15%, 50% {
            -webkit-transform: rotateX(20deg);
            transform: rotateX(20deg);
        }

        75% {
            -webkit-transform: none;
            transform: none;
        }
    }

    .hyper .mouth {
        height: 80px;
        border-radius: 20px 20px 50% 50%;
    }

        /* Mobile-first Responsive======================================= */

