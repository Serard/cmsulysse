
{% block body %}
    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-group">

        <fieldset>
            {{ form_row(form.email) }}
            {{ form_row(form.plainPassword) }}
            {{ form_row(form.lastname) }}
            {{ form_row(form.firstname) }}
            {{ form_row(form.tel) }}
        </fieldset>
        <fieldset>
            <div class="form-group">
                <label class="col-sm-4 control-label">Address</label>
                <div class="col-sm-8">
                    <input id="user_input_autocomplete_address"
                           class="form-control" placeholder="Start typing your address...">
                </div>
            </div>
        </fieldset>
        <fieldset class="disabled">
            {{ form_row(form.address) }}
            {{ form_row(form.postal_code) }}
            {{ form_row(form.city) }}
        </fieldset>

        <section style="display:none">
            {{ form_row(form.is_active) }}
        </section>

            {{ form_rest(form) }}

        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
        <script src='/js/intlTellnput.js'></script>
        <script src="/js/utils.js"></script>
        <script>
            var utilsScriptUrl = '/js/utils.js';
        </script>
        <script src="/js/defaultCountrylp.js"></script>

        <div>
            <input type="submit" value="Envoyer" class="btn btn-template-main"/>
        </div>
    </form>
{% endblock body %}