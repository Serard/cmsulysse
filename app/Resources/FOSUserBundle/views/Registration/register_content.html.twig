
{% block body %}
    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-group">

        <fieldset>
            {{ form_row(form.lastname) }}
            {{ form_row(form.firstname) }}
            {{ form_row(form.email) }}
            {{ form_row(form.plainPassword) }}
        </fieldset>
        <fieldset>
            <div class="form-group">
                <label class="col-sm-4 control-label">Address</label>
                <div class="col-sm-8">
                    <input id="user_input_autocomplete_address"
                           class="form-control" placeholder="Start typing your address...">
                </div>
            </div>
        </fieldset>
        <fieldset class="disabled">
            {{ form_row(form.street_number, { 'id': 'street_number'}) }}
            {{ form_row(form.address, { 'id': 'route'}) }}
            {{ form_row(form.city, { 'id': 'locality'}) }}
            {{ form_row(form.administrative_area, { 'id': 'administrative_area_level_1'}) }}
            {{ form_row(form.postal_code, { 'id': 'postal_code'}) }}
            {{ form_row(form.country, { 'id': 'country'}) }}
            {{ form_row(form.tel) }}

        </fieldset>
        <fieldset>
            <section style="display:none">
                {{ form_row(form.is_active) }}
            </section>
            {{ form_rest(form) }}
            <div>
                <input type="submit" value="Envoyer" class="btn btn-template-main"/>
            </div>
        </fieldset>
    </form>
{% endblock body %}

